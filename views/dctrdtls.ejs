</head>

<body>
  <div class="container" ng-app="myApp" ng-controller="myctrl" ng-init="getdata()">
    <h2>Patient data list</h2>
  <table class="table table-bordered">
    <tr>
      <th>fullName</th>
      <th>Email</th>
      <th>birth</th>
      <th>age</th>
      <th>state</th>
      <th>village</th>
      <th>code</th>
    </tr>
    <tbody>
      <tr ng-repeat="x in studentdata"> 
        <td>{{x.fullname}}</td>
        <td>{{x.Email}}</td>
        <td>{{x.birth}}</td>
        <td>{{x.age}}</td>
        <td>{{x.state}}</td>
        <td>{{x.village}}</td>
        <td>{{x.code}}</td>
     </tr>
      
    </tbody>
    
  </table>
  </div>



  
</body>
